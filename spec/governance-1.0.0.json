{
  "governance": {
    "version": "1.0.0",
    "principles": {
      "stability_bias": "default_to_no_change",
      "determinism_supremacy": "behavior_must_not_depend_on_context",
      "governance_over_code": "rules_define_legitimacy",
      "change_cost_as_feature": "intentional_friction"
    },
    "layers": {
      "constitutional": {
        "elements": [
          "determinism",
          "statelessness",
          "semantic_boundary",
          "capacity_override"
        ],
        "amendable": false
      },
      "policy": {
        "elements": [
          "thresholds",
          "derived_formulas",
          "mode_definitions"
        ],
        "amendable": true,
        "requirements": [
          "formal_RFC",
          "impact_statement",
          "dual_signature",
          "new_major_version",
          "new_audit_hash"
        ]
      },
      "implementation": {
        "elements": [
          "code_structure",
          "tests",
          "documentation"
        ],
        "amendable": true,
        "requirements": [
          "single_maintainer_review",
          "no_behavior_change"
        ]
      },
      "editorial": {
        "elements": [
          "comments",
          "formatting",
          "examples"
        ],
        "amendable": true
      }
    },
    "authority": {
      "maintainer": {
        "can": [
          "merge_non_behavioral_changes",
          "propose_policy_changes"
        ],
        "cannot": [
          "unilaterally_change_policy",
          "bypass_audit"
        ]
      },
      "auditor": {
        "can": [
          "veto_policy_changes",
          "require_evidence",
          "trigger_audit"
        ],
        "cannot": [
          "modify_code",
          "propose_changes"
        ]
      }
    },
    "change_process": {
      "proposal": "ADR_required",
      "review_period_days": 14,
      "approval": "maintainer_plus_auditor",
      "failure_mode": "change_rejected_by_default"
    },
    "wrapper_constraints": {
      "allowed": [
        "input_validation",
        "type_enforcement",
        "logging"
      ],
      "forbidden": [
        "state_retention",
        "external_calls",
        "adaptive_logic",
        "result_modification"
      ],
      "certification": "required_for_official_use"
    },
    "conflict_resolution": {
      "deadlock": "freeze_version",
      "escalation": "steering_committee",
      "bias": "toward_stability"
    },
    "emergency_protocol": {
      "allowed": "safety_restoration_only",
      "duration_days": 30,
      "post_review_required": true
    },
    "succession": {
      "minimum_maintainers": 2,
      "knowledge_transfer_required": true
    }
  }
}
